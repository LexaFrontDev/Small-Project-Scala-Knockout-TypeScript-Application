{
  "name": "knockout-tailwind-typescript",
  "version": "1.0.0",
  "scripts": {
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/tailwind.css --minify",
    "build:ts": "tsc",
    "start": "http-server ./dist -p 8080",
    "watch:css": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/tailwind.css --watch",
    "watch:ts": "tsc --watch"
  },
  "dependencies": {
    "knockout": "^3.5.1"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tailwindcss": "^3.4.14",
    "postcss": "^8.4.30",
    "autoprefixer": "^10.4.15",
    "http-server": "^14.1.1"
  }
}
